<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="DepInf" connectionString="Data Source=localhost\sqlexpress;Initial Catalog=DepInf;Integrated Security=True;MultipleActiveResultSets=True"/>
  </connectionStrings>
  <system.serviceModel>
    <client>
      <endpoint address="net.msmq://localhost/private/Supervisor" binding="netMsmqBinding"
        bindingConfiguration="QueueEndpoint" contract="Supervisor.ISupervisorOps"
        name="QueueEndpoint" />
    </client>
    <bindings>
      <netMsmqBinding>
        <binding name="QueueEndpoint">
          <security mode="None" />
        </binding>
      </netMsmqBinding>
      <netTcpBinding>
        <binding name="TcpBinding" transactionFlow="true">
          <security mode="None" />
        </binding>
      </netTcpBinding>
    </bindings>
    <services>
      <service behaviorConfiguration="BankABehavior" name="BankA.BankAOps">
        <endpoint address="" binding="netTcpBinding" bindingConfiguration="TcpBinding"
          name="TcpOps" contract="BankA.IBankAOps" />
        <endpoint address="mex" binding="mexHttpBinding" name="MetaData"
          contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:8700/BankA/" />
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/BankA/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="BankABehavior">
          <serviceMetadata httpGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
